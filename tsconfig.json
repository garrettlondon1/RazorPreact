{
	"compilerOptions": {
	  "target": "ESNext",
	  "module": "ES6",
	  "moduleResolution": "Bundler",
	  "jsx": "react-jsx",
	  "jsxImportSource": "preact",
	  "lib": [
		"esnext.array",
		"esnext",
		"es2017",
		"dom"
	  ],
	  "declaration": true,
	  "declarationMap": true,
	  "sourceMap": true,
	  "inlineSources": true,
	  "strict": true,
	  "noUnusedLocals": true,
	  "noUnusedParameters": true,
	  "noImplicitReturns": true,
	  "noFallthroughCasesInSwitch": true,
	  "noUncheckedIndexedAccess": true,
	  "noImplicitAny": true,
	  "noImplicitThis": true,
	  "outDir": "wwwroot/js/dist",
	  "skipLibCheck": true,
	  "experimentalDecorators": true,
	  "baseUrl": "."
	},
	"include": [
	  "./**/*.tsx"
	],
	"exclude": [
	  "node_modules",
	  "wwwroot"
	]
  }